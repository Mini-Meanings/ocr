.mynavs{position:absolute;bottom:50rpx;width:100%;display:flex;justify-content:space-around}.mynavs span{width:130rpx;height:130rpx;line-height:130rpx;display:inline-block;border-radius:50%;text-align:center;box-shadow:rgba(38,218,231,0.52) 0 0 58rpx inset;font-size:32rpx;color:#575656;overflow:hidden}.mynavs .ripple{position:relative;overflow:hidden}.mynavs .ripple:after{content:"";display:block;position:absolute;width:100%;height:100%;border-radius:50%;top:0;left:0;pointer-events:none;background-image:radial-gradient(circle, #666 10%, transparent 10.01%);background-repeat:no-repeat;background-position:50%;transform:scale(8, 8);opacity:0;transition:transform .4s,opacity .5s}.mynavs .ripple:active:after{transform:scale(0, 0);opacity:.3;transition:0s}