@import "./../components/preview.wxss";
.contact{height:calc(100vh)}.contact .rmedge{position:absolute;overflow:hidden;width:calc(100vw);height:calc(100vh);box-sizing:border-box;z-index:-2}.contact .temp{width:100%;height:calc(100vh);background:url(http://7xvi3w.com1.z0.glb.clouddn.com/testbg.png) no-repeat center center fixed transparent;background-size:150% 150%;-webkit-filter:blur(30px);filter:blur(30px);z-index:-1;transform:scale(1.1);background-attachment:fixed}.contact .section{margin:30rpx;font-size:34rpx;color:rgba(0,0,0,0.6)}.contact textarea{border:1px solid rgba(0,0,0,0.6);height:150px;border-radius:15rpx;box-sizing:border-box;width:100%;padding:20rpx;color:rgba(0,0,0,0.6)}.contact input{height:60rpx;border:1px solid rgba(0,0,0,0.6);border-radius:15rpx;padding:15rpx}.contact .btns{display:flex;justify-content:flex-end;margin:70rpx 30rpx}.contact span{display:inline-block;color:#666;padding:15rpx 50rpx;border-radius:15rpx;margin-left:30rpx;background:#fff;font-size:34rpx;box-shadow:0 0 5rpx #999}.contact .qusetion{width:130rpx;height:130rpx;line-height:130rpx;display:inline-block;border-radius:50%;text-align:center;box-shadow:rgba(255,255,255,0.52) 0 0 58rpx inset;font-size:32rpx;color:#e4e2e2;position:fixed;bottom:30rpx;right:30rpx;background-color:transparent}